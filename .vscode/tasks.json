{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/src/cqhttp.Cyan.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "build_release",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/src/cqhttp.Cyan.csproj",
                "-c", "Release"
            ]
        }
    ]
}